{
  "name": "@angrybirdman/database",
  "version": "0.1.0",
  "description": "Database schema and migrations for Angry Birdman",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./client": "./client.ts"
  },
  "scripts": {
    "migrate:dev": "cd .. && prisma migrate dev",
    "migrate:deploy": "cd .. && prisma migrate deploy",
    "migrate:reset": "cd .. && prisma migrate reset --force",
    "generate": "cd .. && prisma generate",
    "build": "tsc && resolve-tspaths",
    "studio": "cd .. && prisma studio",
    "seed": "tsx prisma/seed.ts",
    "db:push": "cd .. && prisma db push",
    "db:pull": "cd .. && prisma db pull",
    "format": "cd .. && prisma format",
    "validate": "cd .. && prisma validate"
  },
  "keywords": [
    "angry-birds-2",
    "clan-management",
    "database",
    "prisma"
  ],
  "author": "Angry Birdman Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.11.10",
    "prisma": "^7.1.0",
    "resolve-tspaths": "^0.8.23",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.1.0",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1"
  }
}
